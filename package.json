{
  "name": "jpc-eats",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "sandbox": "ampx sandbox --profile personal",
    "preview": "vite preview",
    "prod-config": "ampx generate outputs --app-id d2d2fy881dt3wt --branch main --profile personal",
    "ios": "npm run prod-config && npm run dev-ios",
    "dev-ios": "npm run build && cap sync && cap copy ios && cap open ios",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.13.2",
    "@aws-amplify/ui-react-geo": "^2.2.13",
    "@aws-sdk/s3-request-presigner": "^3.975.0",
    "@capacitor/app": "^8.0.0",
    "@capacitor/core": "^8.0.1",
    "@capacitor/geolocation": "^8.0.0",
    "@capacitor/ios": "^8.0.1",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.6",
    "@tanstack/react-query": "^5.90.20",
    "aws-amplify": "^6.16.0",
    "aws-lambda": "^1.0.7",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "recharts": "^3.7.0",
    "use-debounce": "^10.1.0"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.19.0",
    "@aws-amplify/backend-cli": "^1.8.1",
    "@capacitor/assets": "^3.0.5",
    "@capacitor/cli": "^8.0.0",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@types/recharts": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@vitejs/plugin-react": "^5.1.2",
    "aws-cdk": "^2.1100.3",
    "aws-cdk-lib": "^2.234.1",
    "constructs": "^10.4.4",
    "dotenv": "^17.2.3",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "typescript": "^5.4.5",
    "vite": "^7.3.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --cache --fix",
      "bash -c 'tsc --noEmit'"
    ],
    "*.json": "prettier --write",
    "*.yml": "prettier --write",
    "*.md": "prettier --write"
  }
}
